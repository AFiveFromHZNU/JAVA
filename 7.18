public class bubble {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	int[] arr={6,3,8,2,9,1};
	System.out.println("排序前数组为：");
	for(int num:arr){
            System.out.print(num+" ");
}
	for(int i=0;i<arr.length-1;i++){
	for(int j=0;j<arr.length-1-i;j++){
	if(arr[j]>arr[j+1]){
	int temp=arr[j];
	arr[j]=arr[j+1];
	arr[j+1]=temp;
	}

}
	}
	System.out.print("\n排序后数组为 ：\n");
	for(int num:arr) {
		System.out.print(num+" ");
	}

	}
}
